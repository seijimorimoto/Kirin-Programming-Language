Main {
  public var i, j : int;

  public_func void uno(a : int) {
    var i : int;
    i = a*2;
    if (i < a + 4) {
      uno(a + 1);
    }
    print(i);
  }

  public_func int dos(b : int) {
    b = b*i + j;
    return b*2;
  }

  public_func void main() {
    i = 2;
    j = i*2 - 1;
    uno(j);
    print(i + dos(i + j));
  }
}