class Clase1 {
  public var num1, num2: int;
  
  constructor() {
      num1 = 0;
      num2 = 0;
  }

  public_func void display() {
    print(num1, "-", num2);
  }

  public_func independent void addOneToObj(c : Clase1) {
    c.num1 = c.num1 + 1;
    c.num2 = c.num2 + 1;
  }
}

class Clase2 {
  public independent var obj : Clase1;
  public var num3 : int;
  
  constructor() {
      num3 = 0;
      obj = new();
  }

  public_func void increaseByOne() {
    obj.num1 = obj.num1 + 1;
    obj.num2 = obj.num2 + 1;
    num3 = num3 + 1;
  }

  public_func void display() {
    print(obj.num1, "-", obj.num2, "-", num3);
  }
}

Main {
  public_func independent void main() {
    var x, y : Clase2;
    x = new();
    y = new();
    
    call x.increaseByOne();
    call Clase1.addOneToObj(Clase2.obj);
    call Clase1.addOneToObj(y.obj);

    call x.display();
    call y.display();
  }
}
